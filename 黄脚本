    pcall(function()  -- åŒ…è£¹SetCoreè°ƒç”¨ï¼Œé¿å…å› æ¸¸æˆé™åˆ¶å¯¼è‡´æŠ¥é”™
        StarterGui:SetCore("SendNotification", {
            Title = "é»„åå•è®¤è¯",
            Text = "ç©å®¶:" .. localPlayer.Name .. "ï¼Œæ£€æµ‹å®Œæ¯•é»„åå•ç©å®¶ï¼Œç¨åä¼šåŠ è½½è„šæœ¬",
            Duration = 7,
        })
    end)
else
    localPlayer:Kick("ä½ æ²¡æœ‰è¢«åŠ å…¥é»„åå•ï¼Œè¯·åŠ ç¾¤543121803")
    return  -- éé»„åå•ç©å®¶ç›´æ¥é€€å‡ºï¼Œä¸åŠ è½½åç»­å†…å®¹
