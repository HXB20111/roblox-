    StarterGui:SetCore("SendNotification", {
        Title = "é»„è„šæœ¬",
        Text = "æ¬¢è¿ä½¿ç”¨ï¼ŒåŠŸèƒ½å·²å°±ç»ª",
        Duration = 4,
        Icon = "rbxassetid://118894209472715"
    })
end)

