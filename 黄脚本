                OrionLib:MakeNotification({Name = "æˆåŠŸ", Content = "å¤§å¸é©¬è„šæœ¬åŠ è½½å®Œæˆ", Time = 3})
            else
                OrionLib:MakeNotification({Name = "å¤±è´¥", Content = "é”™è¯¯: " .. err, Time = 3})
            end
        end)
    end,
    Color = Color3.fromRGB(255, 78, 91)
})

